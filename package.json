{
  "name": "can-i-search",
  "displayName": "Can I Search",
  "description": "You Can Search",
  "publisher": "xiaogang",
  "icon": "images/logo.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:can-i-search.web",
    "onCommand:can-i-search.npm",
    "onCommand:can-i-search.translate",
    "onCommand:can-i-search.addConsole",
    "onCommand:can-i-search.deleteConsole"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "can-i-search.web",
        "title": "Open Web"
      },
      {
        "command": "can-i-search.npm",
        "title": "Open Npm"
      },
      {
        "command": "can-i-search.translate",
        "title": "Open Translate"
      },
      {
        "command": "can-i-search.console",
        "title": "Insert Console"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "can-i-search.web",
          "group": "group@1"
        },
        {
          "command": "can-i-search.npm",
          "group": "group@2"
        },
        {
          "command": "can-i-search.translate",
          "group": "group@3"
        }
      ],
      "explorer/context": [
        {
          "command": "can-i-search.web",
          "group": "group@1"
        },
        {
          "command": "can-i-search.npm",
          "group": "group@2"
        },
        {
          "command": "can-i-search.translate",
          "group": "group@3"
        }
      ]
    },
    "keybindings": [
      {
        "when": "editorTextFocus",
        "command": "can-i-search.web",
        "key": "shift+alt+w"
      },
      {
        "when": "editorTextFocus",
        "command": "can-i-search.npm",
        "key": "shift+alt+n"
      },
      {
        "when": "editorTextFocus",
        "command": "can-i-search.translate",
        "key": "shift+alt+t"
      },
      {
        "when": "editorTextFocus",
        "command": "can-i-search.addConsole",
        "key": "shift+alt+l"
      },
      {
        "when": "editorTextFocus",
        "command": "can-i-search.deleteConsole",
        "key": "shift+alt+d"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "open": "^8.4.0",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  }
}
